{
	"name": "ashanet/rbruteforce",
	"description": "CakePHP 3 Plugin for Protection Against BruteForce Attacks",
	"type": "cakephp-plugin",
	"keywords": ["cakephp", "brute force", "security"],
	"homepage": "https://github.com/ashanet/rBruteForce",
	"license": "MIT",
	"authors": [
		{
			"version": "dev-master",
			"name": "ashanet",
			"homepage": "http://ashanet.ir",
			"role": "Author"
		}
	],
	"support": {
		"issues": "https://github.com/ashanet/rBruteForce/issues",
		"source": "https://github.com/ashanet/rBruteForce"

	},
	"require": {
		"cakephp/cakephp": "^3.7",
		"cakephp/plugin-installer": "*"
	},
	"require-dev": {
		"phpunit/phpunit": "^5.7|^6.0",
		"cakephp/migrations": "@stable",
		"cakephp/debug_kit": "~3.0"
	},
	"autoload": {
		"psr-4": {
			"RBruteForce\\": "src",
			"RBruteForce\\Test\\Fixture\\": "tests\\Fixture"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Cake\\Test\\": "vendor/cakephp/cakephp/tests",
			"RBruteForce\\Test\\": "tests"
		}
	}
}
